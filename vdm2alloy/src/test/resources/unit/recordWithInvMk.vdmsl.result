{ nat = { i:Int | gte[i,0]}}
}

sig R{
	f: one H, \n\th: one nat, \n\ty: one Bool
}{\n\t(  let a = y, c = f | \n\t\t(\n\t\t\t(a != True)\n\t\t and \n\t\t\t(c != B)\n\t\t)\n\t)\n}
"