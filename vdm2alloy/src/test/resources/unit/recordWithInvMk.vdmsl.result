"module recordWithInvMk.vdmsl

open util/relation
open util/boolean
open vdmutil

/************************   H   ************************/
one sig A{}
one sig B{}
sig H in univ{}
fact HType{
H = { y : A + B}
}

/************************   R   ************************/
sig nat in univ{}
